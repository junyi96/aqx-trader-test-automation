# Environment configuration template
# Copy this file to .env and fill in your values

# Test Environment (development, staging, production)
TEST_ENV=development

# Application Credentials
# WARNING: Never commit .env file with real credentials!
AQX_USERNAME=your_username_here
AQX_PASSWORD=your_password_here

# Browser Settings
BROWSER=chromium  # chromium, firefox, webkit
HEADLESS=false
SLOW_MO=0  # milliseconds to slow down operations

# URLs (optional - uses defaults from config if not set)
# STAGING_URL=https://staging.aqxtrader.aquariux.com
# PROD_URL=https://aqxtrader.aquariux.com